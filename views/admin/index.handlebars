  <div id="content-wrapper">

    <div class="container-fluid">

      <!-- Breadcrumbs-->
      <ol class="breadcrumb">
        <li class="breadcrumb-item">
          <a href="#">Dashboard</a>
        </li>
        <li class="breadcrumb-item active">Overview</li>
      </ol>
      <!-- DataTables Example -->
      <div class="card mb-3">
        <div class="card-header">
          <i class="fas fa-table"></i>
          ToDo List
        </div>
        <div class="card-body">
          <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
              <thead>
                <tr>
                  <th>{{texts "email"}}</th>
                  <th>{{texts "first"}} {{texts "name"}}</th>
                  <th>{{texts "last"}} {{texts "name"}}</th>
                  <th>{{texts "creation"}}</th>
                  <th>{{texts "content"}}</th>
                  <th>{{texts "actions"}}</th>
                </tr>
              </thead>
              <tfoot>
                <tr>
                  <th>{{texts "email"}}</th>
                  <th>{{texts "first"}} {{texts "name"}}</th>
                  <th>{{texts "last"}} {{texts "name"}}</th>
                  <th>{{texts "creation"}}</th>
                  <th>{{texts "content"}}</th>
                  <th>{{texts "actions"}}</th>
                </tr>
              </tfoot>
              <tbody>
                {{#each list}}
                <tr>
                  <td>{{email}}</td>
                  <td>{{first_name}}</td>
                  <td>{{last_name}}</td>
                  <td>{{creation}}</td>
                  <td>{{content}}</td>
                  <td>
                    <button class="btn btn-danger">
                      <i class="fa fa-times" aria-hidden="true"></i>
                    </button>
                  </td>
                </tr>
                {{/each}}


              </tbody>
            </table>
          </div>
        </div>
        <div class="card-footer small text-muted">Updated yesterday at 11:59 PM</div>
      </div>

